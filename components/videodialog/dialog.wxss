.videoDialogCover {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100vh;
    background-color: rgba(0,0,0,0.3);
    z-index: 1000;
}

.videoDialog {
    position: fixed;
    left: 50%;
    top: 50%;
    transform: translateX(-50%) translateY(-50%);
    width: 650rpx;
    padding: 50rpx 70rpx;
    text-align: center;
    border-radius: 10px;
    background-color: #fff;
    box-sizing: border-box;
    z-index: 1001;
}

.videoDialog .content {
    margin-top: 20rpx;
    color: #666666;
    font-size: 42rpx;
}

.videoDialog .button {
    display: inline-flex;
    flex-direction: row;
    justify-content: center;
    align-items: center;
    height: 120rpx;
    line-height: 120rpx;
    margin-top: 80rpx;
    padding: 0 70rpx;
    color: #fff;
    font-size: 46rpx;
    border-radius: 70rpx;
    background-color: rgb(252,179,28);
    z-index: 1001;
}

.button image {
    width: 44rpx;
    height: 44rpx;
    margin-right: 10rpx;
    display: block;
}